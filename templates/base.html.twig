<!DOCTYPE html>
<html lang="pt">
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        <div class="container">
            {% include "_menu_desktop.html.twig" %}
            {% include "_flash_alerts.html.twig" %}
        </div>
        <div class="container">
            {% block body %}{% endblock %}
        </div>
        <div class="containter" style="margin-bottom: 110px;">
            {% include "_menu_mobile.html.twig" %}
        </div>
    </body>
</html>
